{
  "name": "basebase-js",
  "version": "0.1.6",
  "description": "BaseBase SDK for Javascript/Typescript - Firebase-like API for BaseBase server",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./auth": {
      "import": "./dist/auth.esm.js",
      "require": "./dist/auth.cjs",
      "types": "./dist/auth.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:types": "tsc --emitDeclarationOnly",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts,.js",
    "lint:fix": "eslint src --ext .ts,.js --fix",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "basebase",
    "firebase",
    "backend",
    "database",
    "javascript",
    "typescript",
    "sdk",
    "api-client"
  ],
  "author": "BaseBase Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/grenager/basebase-js-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/grenager/basebase-js-sdk/issues"
  },
  "homepage": "https://github.com/grenager/basebase-js-sdk#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/jest": "^29.5.8",
    "@types/js-cookie": "^3.0.6",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.6.1",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "js-cookie": "^3.0.5"
  },
  "peerDependencies": {
    "typescript": ">=4.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
